        <script>
            // –û—Ç—Ä–∏–º—É—î–º–æ –≤—Å—ñ –∫–Ω–æ–ø–∫–∏ –≤—ñ–¥–∫—Ä–∏—Ç—Ç—è
const openButtons = document.querySelectorAll('.index-open-btn');
const closeButtons = document.querySelectorAll('.index-close-btn');

// –î–æ–¥–∞—î–º–æ –ø–æ–¥—ñ—ó –¥–ª—è –∫–æ–∂–Ω–æ—ó –∫–Ω–æ–ø–∫–∏ –≤—ñ–¥–∫—Ä–∏—Ç—Ç—è
openButtons.forEach(button => {
    button.addEventListener('click', () => {
        const targetId = button.getAttribute('data-target'); // –û—Ç—Ä–∏–º—É—î–º–æ ID –ø–æ—Ç—Ä—ñ–±–Ω–æ–≥–æ –∫–æ–Ω—Ç–µ–π–Ω–µ—Ä–∞
        const container = document.getElementById(targetId);
        if (container) {
            container.style.display = 'flex';
        }
    });
});

// –î–æ–¥–∞—î–º–æ –ø–æ–¥—ñ—ó –¥–ª—è –∫–Ω–æ–ø–æ–∫ –∑–∞–∫—Ä–∏—Ç—Ç—è
closeButtons.forEach(button => {
    button.addEventListener('click', () => {
        const container = button.parentElement;
        container.style.display = 'none';
    });
});

        </script>

